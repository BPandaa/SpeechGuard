{
    "manifest_version": 3,
    "name": "SpeechGuard",
    "version": "1.0.0",
    "description": "A chrome extension to detect hate speech",
    "content_scripts": [
        {
          "js": ["/content.js"],
          "matches": [
            "https://twitter.com/*"
          ]
        }
      ],
      "icons": {
        "16": "logo16.png",
        "48": "logo48.png",
        "128": "logo128.png"
      }
  }